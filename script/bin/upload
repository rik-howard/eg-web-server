#!/bin/bash
##
# $ upload <fin> <fon>
##

if test -f "$1"
then
    if ! test -e "$2"
    then mkfir -p "$2"
    fi
    leaf=$(basename "$1")
    if test -f "$2/$leaf"
    then
        echo "file exists:"
        ls -l "$2/$leaf"
        exit 2
    else
        cp -pr "$1" "$2"
        ls -l "$2/$leaf"
    fi
else
    echo "not file: $1"
    exit 1
fi
